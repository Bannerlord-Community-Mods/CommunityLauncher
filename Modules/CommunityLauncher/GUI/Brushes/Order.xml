<Brushes>
  <Brush Name="Order.Troop.Background">
    <Layers>
      <BrushLayer Name="Default" Sprite="Order\OrderTroopBackground" />
      <BrushLayer Name="Highlight" Sprite="Order\OrderItemHighlight" HueFactor="-30" SaturationFactor="70" ExtendLeft="-10" ExtendRight="-10" ExtendTop="-10" ExtendBottom="-10" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="1" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="0.8" ColorFactor="0.5" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Order.Item.Background">
    <Layers>
      <BrushLayer Name="Default" Sprite="Order\OrderItem" />
      <BrushLayer Name="Highlight" Sprite="Order\OrderItemHighlight" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
      <Style Name="Active">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="1" HueFactor="-30" SaturationFactor="70" />
      </Style>
      <Style Name="PartiallyActive">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="1" HueFactor="-10" SaturationFactor="0" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Order.Troop.Icon">
    <Layers>
      <BrushLayer Name="Default" Sprite="Order\TroopIcons\Infantry" />
    </Layers>
    <Styles>
      <Style Name="Infantry">
        <BrushLayer Name="Default" Sprite="Order\TroopIcons\Infantry" />
      </Style>
      <Style Name="Ranged">
        <BrushLayer Name="Default" Sprite="Order\TroopIcons\Ranged" />
      </Style>
      <Style Name="Cavalry">
        <BrushLayer Name="Default" Sprite="Order\TroopIcons\Cavalry" />
      </Style>
      <Style Name="HorseArcher">
        <BrushLayer Name="Default" Sprite="Order\TroopIcons\HorseArcher" />
      </Style>
      <Style Name="Mixed">
        <BrushLayer Name="Default" Sprite="Order\TroopIcons\Generic" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Order.Troop.MoraleFillBar">
    <Layers>
      <BrushLayer Name="EmptyFill" Sprite="BlankWhiteSquare_9" ColorFactor="0.8" Color="#00000000" />
      <BrushLayer Name="DefaultFill" Sprite="BlankWhiteSquare_9" ColorFactor="1" Color="#FFD98755" />
      <BrushLayer Name="ChangeFill" Sprite="BlankWhiteSquare_9" ColorFactor="1.5" Color="#00000000" />
    </Layers>
    <Styles>
      <Style Name="Default">
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Order.Transfer.Troop.Background">
    <Layers>
      <BrushLayer Name="Default" Sprite="Order\MinimalOrderTroop" />
      <BrushLayer Name="Highlight" Sprite="Order\OrderItemHighlight" HueFactor="-30" SaturationFactor="70" ExtendLeft="-10" ExtendRight="-10" ExtendTop="-5" ExtendBottom="-10" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.4" />
        <BrushLayer Name="Highlight" AlphaFactor="0" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Highlight" AlphaFactor="1" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Order.Siege.Deployment.DeployPoint">
    <Layers>
      <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9" Color="#000000FF" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" AlphaFactor="0.5" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="0.3" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" AlphaFactor="0.5" Color="#B58127FF" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" AlphaFactor="0.3" Color="#9B6F22FF" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" AlphaFactor="0.5" Color="#B26017FF" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Order.Siege.Deployment.MachineItem">
