  <Brush Name="CharacterCreation.GainedProperty.Active">
    <Layers>
      <BrushLayer Name="Active" Sprite="BlankWhiteSquare" Color="#ACAC48FF"/>
    </Layers>
    <Styles>
      <Style Name="Default" AnimationToPlayOnBegin="GainedSkillAnimation">
        <BrushLayer Name="Active" AlphaFactor="1" />
      </Style>
    </Styles>
    <Animations>
      <Animation Name="GainedSkillAnimation" Duration="2.2" Loop="true">
        <AnimationProperty PropertyName="AlphaFactor" LayerName="Active">
          <KeyFrame Time="0.0" Value="1.0" />
          <KeyFrame Time="0.4" Value="1.0" />
          <KeyFrame Time="0.6" Value="0" />
          <KeyFrame Time="0.9" Value="0" />
          <KeyFrame Time="1.1" Value="1.0" />
          <KeyFrame Time="1.5" Value="1.0" />
          <KeyFrame Time="1.7" Value="0" />
          <KeyFrame Time="2" Value="0" />
          <KeyFrame Time="2.2" Value="1.0" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>
  
