  <Brush Name="ButtonBrush" FontColor="#FAF4DEFF" TransitionDuration="0">
    <Layers>
      <BrushLayer Name="Default" Sprite="button_canvas_9" OverlayMethod="CoverWithTexture" OverlaySprite="button_marble" />
      <BrushLayer Name="Overlay" Sprite="button_frame_9" />
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#FAF4DEFF">
        <BrushLayer Name="Default" ColorFactor="1.0" />
        <BrushLayer Name="Overlay" />
      </Style>
      <Style Name="Hovered" AnimationToPlayOnBegin="HoverAnimation">
        <BrushLayer Name="Default" ColorFactor="1.25" />
        <BrushLayer Name="Overlay" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.75" />
        <BrushLayer Name="Overlay" />
      </Style>
      <Style Name="Disabled" FontColor="#FAF4DEAA">
        <BrushLayer Name="Default" ColorFactor="0.30" />
        <BrushLayer Name="Overlay" />
      </Style>
    </Styles>
    <Animations>
      <Animation Name="HoverAnimation" Duration="20.0" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0" Value="1.25" />
        </AnimationProperty>
        <AnimationProperty LayerName="Default" PropertyName="OverlayXOffset">
          <KeyFrame Time="0" Value="0" />
          <KeyFrame Time="10" Value="512" />
          <KeyFrame Time="20" Value="1024" />
        </AnimationProperty>
        <AnimationProperty LayerName="Default" PropertyName="OverlayYOffset">
          <KeyFrame Time="0" Value="0" />
          <KeyFrame Time="20" Value="512" />
        </AnimationProperty>
      </Animation>
    </Animations>
