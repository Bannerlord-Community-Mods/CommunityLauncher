  <Brush Name="MPHUD.GoldAmount.Text" Font="Galahad_Numbers_Bold" TextHorizontalAlignment="Center" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontColor="#FFFFFFFF" TextOutlineColor="#000000FF" TextBlur="0.3" TextOutlineAmount="0.1" TextGlowRadius="0.0" FontSize="40" AnimationToPlayOnBegin="ValueChangeAnimation" />
      <Style Name="Negative" AnimationToPlayOnBegin="NegativeValueChangeAnimation" />
      <Style Name="Positive" AnimationToPlayOnBegin="PositiveValueChangeAnimation" />
    </Styles>
    <Animations>
      <Animation Name="PositiveValueChangeAnimation" Duration="0.6" Loop="false">
        <AnimationProperty PropertyName="FontColor">
          <KeyFrame Time="0.0" Value="#FFFFFFFF" />
          <KeyFrame Time="0.01" Value="#FFDF00FF" />
        </AnimationProperty>
        <AnimationProperty PropertyName="TextBlur">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.01" Value="0.5" />
        </AnimationProperty>
        <AnimationProperty PropertyName="TextGlowRadius">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.01" Value="0.25" />
        </AnimationProperty>
      </Animation>
      <Animation Name="NegativeValueChangeAnimation" Duration="0.6" Loop="false">
        <AnimationProperty PropertyName="FontColor">
          <KeyFrame Time="0.0" Value="#FFFFFFFF" />
          <KeyFrame Time="0.01" Value="#F442EEFF" />
        </AnimationProperty>
        <AnimationProperty PropertyName="TextBlur">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.01" Value="0.5" />
        </AnimationProperty>
        <AnimationProperty PropertyName="TextGlowRadius">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.01" Value="0.25" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>

