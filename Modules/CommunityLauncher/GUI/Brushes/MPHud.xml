  <Brush Name="MPHUD.MoraleWidget.Item">
    <Layers>
      <BrushLayer Name="Default" Sprite="MPHud\star_without_glow" AlphaFactor="0.8" Color="#BB7312FF"/>
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="IncreaseAnim" AnimationToPlayOnBegin="IncreaseAnimation">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="IsCriticalAnim" AnimationToPlayOnBegin="IsCriticalAnimation">
        <BrushLayer Name="Default" />
      </Style>
    </Styles>
    <Animations>
      <Animation Name="IncreaseAnimation" Duration="1.5" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0.0" Value="1" />
          <KeyFrame Time="0.15" Value="1.2" />
          <KeyFrame Time="0.25" Value="1.3" />
          <KeyFrame Time="0.4" Value="1.4" />
          <KeyFrame Time="0.5" Value="1.6" />
        </AnimationProperty>
      </Animation>
      <Animation Name="IsCriticalAnimation" Duration="1.5" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0.0" Value="1" />
          <KeyFrame Time="0.1" Value="3.5" />
          <KeyFrame Time="0.75" Value="4" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>
